<h3 class="ui header">{{t "forms.i-i-s-proba9-рег-результ-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proba9-рег-результ-e.датРегРезСобес-caption"}}</label>
    {{flexberry-simpledatetime
      type="date"
      readonly=readonly
      value=model.датРегРезСобес
      class=(if (v-get validationModel "датРегРезСобес" "isInvalid") "error")
      data-test-i-i-s-proba9-рег-результ-e-датРегРезСобес=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "датРегРезСобес" "message")}}
  </div>
  <div class="field" data-test-i-i-s-proba9-рег-результ-e-итогСобес="true">
    <label>{{t "forms.i-i-s-proba9-рег-результ-e.итогСобес-caption"}}</label>
    {{flexberry-dropdown
      items=(flexberry-enum "i-i-s-proba9-итог-собес")
      value=model.итогСобес
      class=(if (v-get validationModel "итогСобес" "isInvalid") "error")
      readonly=readonly
    }}
    {{flexberry-validationmessage error=(v-get validationModel "итогСобес" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.оценкаКандид
      class=(if (v-get validationModel "оценкаКандид" "isInvalid") "error")
      label=(t "forms.i-i-s-proba9-рег-результ-e.оценкаКандид-caption")
      data-test-i-i-s-proba9-рег-результ-e-оценкаКандид=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "оценкаКандид" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.крКомИлиОбсРеш
      class=(if (v-get validationModel "крКомИлиОбсРеш" "isInvalid") "error")
      label=(t "forms.i-i-s-proba9-рег-результ-e.крКомИлиОбсРеш-caption")
      data-test-i-i-s-proba9-рег-результ-e-крКомИлиОбсРеш=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "крКомИлиОбсРеш" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proba9-рег-результ-e.сотрудники-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.сотрудники
      displayAttributeName="фИОСотруд"
      autocomplete=true
      relationName="сотрудники"
      projection="СотрудникиL"
      title=(t "forms.i-i-s-proba9-рег-результ-e.сотрудники-caption")
      readonly=readonly
      componentName="сотрудникиLookup"
      class=(if (v-get validationModel "сотрудники" "isInvalid") "error")
      data-test-i-i-s-proba9-рег-результ-e-сотрудники=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "сотрудники" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proba9-рег-результ-e.организация-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.организация
      displayAttributeName="наименование"
      autocomplete=true
      relationName="организация"
      projection="ОрганизацияL"
      title=(t "forms.i-i-s-proba9-рег-результ-e.организация-caption")
      readonly=readonly
      componentName="организацияLookup"
      class=(if (v-get validationModel "организация" "isInvalid") "error")
      data-test-i-i-s-proba9-рег-результ-e-организация=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "организация" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-proba9-рег-результ-e.планирСобес-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.планирСобес
      displayAttributeName="иметьПриСебе"
      autocomplete=true
      relationName="планирСобес"
      projection="ПланирСобесL"
      title=(t "forms.i-i-s-proba9-рег-результ-e.планирСобес-caption")
      readonly=readonly
      componentName="планирСобесLookup"
      class=(if (v-get validationModel "планирСобес" "isInvalid") "error")
      data-test-i-i-s-proba9-рег-результ-e-планирСобес=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "планирСобес" "message")}}
  </div>
</form>
